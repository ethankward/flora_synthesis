{
    "builds": [
        {
        "src": "flora_synthesis/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    },
        {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "flora_synthesis/wsgi.py"
        }
    ]
}